{
    "name": "Ruipeng Theme Roller",
    "version": "0.0.9",
    "description": "定制所有设计符号的元素UI和实时预览",
    "web_accessible_resources": ["entry.js"],
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },
    "icons": {
        "128": "icon.png"
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Ruipeng ui 主题元素"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "manifest_version": 2,
    "permissions": ["activeTab", "storage", "https://*.ele.me/", "https://*.elenet.me/", "http://localhost/"]
}
